<Bundle>
<Group name='ubuntu'>
    <Package name='cups'/>
    <Package name='cups-browsed'/>
    <Group name='ubuntu-xenial'>
        <Package name='cups-pdf'/>
    </Group>
    <Group name='ubuntu-xenial' negate='true'>
        <Package name='printer-driver-cups-pdf'/>
    </Group>
    <Service name='cups'/>
    <Service name='cups.path'/>
    <Service name='cups.socket'/>
    <Service name='cups-browsed'/>
    <Path name='/etc/cups/cupsd.conf'/>
    <Path name='/etc/ssl/cups.pem'/>
    <Path name='/etc/ssl/private/cups.key'/>
    <Path name='/etc/cups/ssl/server.crt'/>
    <Path name='/etc/cups/ssl/server.key'/>
</Group>
</Bundle>
